{
  "name": "rnative_musicplayer_app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:android-build-bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "android:android-build-apk-release": "cd android && gradlew assembleRelease",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "android": "react-native run-android",
    "android:build": "npm run android:android-build-bundle && npm run android:android-build-apk-release",
    "android:install-build": "adb install android/app/build/outputs/apk/release/app-release.apk"
  },
  "react-native": {
    "assets": [
      "./assets/fonts"
    ]
  },
  "dependencies": {
    "music-metadata": "^11.6.0",
    "react": "19.1.0",
    "react-native": "0.80.1",
    "react-native-audio-pro": "^9.9.1",
    "react-native-fs": "^2.20.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-safe-area-context": "^5.5.1",
    "react-native-sqlite-storage": "^6.0.1",
    "react-native-use-websocket": "^0.2.6",
    "react-native-webrtc": "^124.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "19.0.0",
    "@react-native-community/cli-platform-android": "19.0.0",
    "@react-native-community/cli-platform-ios": "19.0.0",
    "@react-native/babel-preset": "0.80.1",
    "@react-native/eslint-config": "0.80.1",
    "@react-native/metro-config": "0.80.1",
    "@react-native/typescript-config": "0.80.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-native-sqlite-storage": "^6.0.5",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
